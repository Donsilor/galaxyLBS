#wrap{background: url("../images/commonBg.jpg") no-repeat center/cover;position: fixed;top: 0;right: 0;bottom: 0;left: 0;}
#space {width: 100%;}
#warp {position: absolute;z-index: 9;bottom: 0;left: 0;right: 0;margin: 20px auto;color: rgba(209, 255, 255, 1);border: 2px solid;padding: 1em;width: 10em;text-align: center;font-weight: 700;font-size: 1.2em;display: inline-block;text-decoration: none;background: rgba(0, 0, 0, 0.8)}


/* 通用头部 */
#fixed-top{position: fixed;top: 0;right: 0;left: 0;height: 8rem;z-index: 999;}
#fixed-top > .logo{height: 100%;margin-right: 2.5%;}
#fixed-top > .logo img{display: block;height: 100%;}
.fixed-top-nav li{float: left;padding: 2rem 0 2rem 3rem;transition: all .3s;}
.fixed-top-nav li.link_1{background: url("../images/nav_1.png") no-repeat top left/auto 100%;}
.fixed-top-nav li.link_2{background: url("../images/nav_2.png") no-repeat top left/auto 100%;}
.fixed-top-nav li.link_3,
.fixed-top-nav li.link_4{background: url("../images/nav_3.png") no-repeat top left/auto 100%;}
.fixed-top-nav li.link_5{background: url("../images/nav_4.png") no-repeat top left/auto 100%;}
.fixed-top-nav li.link_6{background: url("../images/nav_5.png") no-repeat top left/auto 100%;}
.fixed-top-nav li.link_7{background: url("../images/nav_6.png") no-repeat top left/auto 100%;}
.fixed-top-nav li:hover,
.fixed-top-nav li.active{background: url("../images/nav_active.png") no-repeat top left/auto 100%;text-shadow: 0 0 10px #fff;}
.fixed-top-nav li a{display: block;font-size: 1.2rem;font-weight: bold;padding: 1rem .5rem 1rem 2rem;}
.fixed-top-operate{margin-top: 2.5rem;margin-right: 2.5%;}
.fixed-top-operate > div{float: left;margin-left: .5rem;position: relative;}
.fixed-top-operate > div > a{display: block;width: 3rem;}
.fixed-top-operate > div > a img{display: block;width: 100%;}
.fixed-top-operate .subMenu{display: none;position: absolute;width: 6.5rem;top: 100%;left: 50%;transform: translateX(-50%);padding-top: 1.5rem;}
.fixed-top-operate .subMenu:before{content: '';position: absolute;top: .625rem;left: 0;width: 0;height: 0;border-width:0 3.25rem .875rem;border-style:solid;border-color:transparent transparent #1f6b79;}
.fixed-top-operate .subMenu ul{background-color: #1f6b79;border-radius: 5px;text-align: center;padding: .5rem 1rem;}
.fixed-top-operate .subMenu ul li{padding: .2rem 0;}
.fixed-top-operate .subMenu ul li:not(:last-of-type){border-bottom: 1px dashed #ccc;}
.fixed-top-operate div:hover .subMenu{display: block;}
.fixed-top-operate div:hover .subMenu a{display: block;}

/* 通用左侧下拉菜单 */
#fixed-side{position: fixed;top: 8rem;bottom: 5vh;left: 0;width: 245px;transition: all .3s;z-index: 2;}
.fixed-side-nav{width: 95%;height: 65vh;margin: 4rem auto 0;background: url("../images/leftDiv_bg.png") no-repeat center/100% 100%;padding: 1.5rem 5% 0;position: relative;float: right;z-index: 100;}
.fixed-side-nav .searchDiv{width: 100%;height: 2rem;border: 1px solid #ccc;border-radius: 5px;}
.fixed-side-nav .searchDiv input{width: 80%;height: 100%;padding-left: .5rem;font-size: .8rem;}
.fixed-side-nav .searchDiv button{float: right;width: 18%;height: 100%;background: url("../images/searchIcon.png") no-repeat center/auto 70%;}
.fixed-side-nav .sideList{height: 55vh;margin-top: 1rem;overflow-y: scroll;overflow-x: scroll;}
.fixed-side-nav .sideList::-webkit-scrollbar{width: .5rem;height: .5rem;background-color: rgba(255,255,255,.1);}
.fixed-side-nav .sideList::-webkit-scrollbar-thumb{background-color: rgba(255,255,255,.2);}
.fixed-side-nav .sideList .item{width: calc(100% + 20px);font-size: 1rem;line-height: 1.8;white-space: nowrap;}
.fixed-side-nav .sideList .item .switchBtn{display: inline-block;width: .9rem;height: .9rem;background: url("../images/icon_shrink.png") no-repeat center/contain;}
.fixed-side-nav .sideList .item.show .switchBtn{background: url("../images/icon_unfold.png") no-repeat center/contain;}
.fixed-side-nav .sideList input[type=checkbox]{margin: 0 .3rem;vertical-align: middle;}
.fixed-side-nav .sideList ul ul > li{padding-left: 1rem;}
.fixed-side-nav .sideList ul ul ul > li{padding-left: 2rem;}
.fixed-side-nav .toggleBtn{position: absolute;top: 50%;right: 1px;transform: translateY(-50%);width: 16px;height: 70px;border-radius: 3px;transition: all .3s;background: url("../images/toggle_right.png") no-repeat center/100% 100%;}
.fixed-side-nav .toggleBtn.unfold{background: url("../images/toggle_left.png") no-repeat center/100% 100%;}

/* 系统管理 - 左侧菜单 */
.systemManage-nav{width: 8vw;margin: 5rem auto 0;}
.systemManage-nav a{display: block;text-align: center;line-height: 2.5;background: url("../images/column.png") no-repeat center/contain;color: #1f6b7b;}
.systemManage-nav li{margin-bottom: 1.5rem;}
.systemManage-nav li > div{width: 80%;margin-left: 20%;display: none;}
.systemManage-nav li > div a{font-size: .9rem;line-height: 2.6;margin: 1rem 0;}
.systemManage-nav li.active > div{display: block;}
.systemManage-nav li.active a{background: url("../images/curColumn.png") no-repeat center/contain;color: #fff;}
.systemManage-nav li.active a.active{background: url("../images/curColumn_menu.png") no-repeat center/contain;color: #1f6b7b;}






/* 通用主体 */
#fixed-main-wrap{position: fixed;top: 8rem;right: 0;bottom: 5vh;left: 245px;transition: all .3s;}

#abs-level3menu{position: absolute;top: 0;right: 0;left: 0;height: 2.5rem;padding-left: 3%;padding-right: 1.7%;z-index: 2;}
#abs-level3menu .filterDiv{background: url("../images/checkDiv_bg.png") no-repeat center/contain;line-height: 2.5rem;padding: 0 2rem;}
#abs-level3menu .filterDiv input{vertical-align: text-bottom;margin-right: .5rem;}
#abs-level3menu .filterDiv label:not(:first-of-type){margin-left: 1rem;}

#abs-main{position: absolute;top: 2.5rem;right: 0;bottom: 0;left: 0;padding-left: 3%;padding-right: 1.7%;}







/* 实时监控 - 右下角监控弹窗 */
.warnPopup{position: fixed;right: 1.875rem;bottom: 1.875rem;width: 20rem;height: 10rem;background: url("../images/popupBg.png") no-repeat center/100% 100%;}
.warnPopup > .decorate{position: absolute;bottom: -1.875rem;left: 50%;transform: translateX(-50%);width: 24rem;}
.warnPopup > .decorate img{display: block;width: 100%;}
.warnSwiper{margin: 2rem 1.5rem auto;}
.warnSwiper .descript{display: block;text-overflow: ellipsis;white-space: nowrap;overflow: hidden;text-align: center;font-size: .875rem;}
.warnSwiper .descript > i{display: inline-block;width: .625rem;height: .625rem;border-radius: 50%;background-color: #f1b013;margin-right: .5rem;}
.warnSwiper .time{text-align: center;font-size: .75rem;line-height: 2.5;}
.warnSwiper .viewBtn{display: block;background-color: #1f6b7b;width: 5.625rem;line-height: 2rem;font-size: .75rem;text-align: center;border-radius: 3px;margin: auto;}

.warnSwiper .swiper-button-prev,
.warnSwiper .swiper-button-next{width: 1rem;height: 2rem;top: auto;bottom: 0;}
.warnSwiper .swiper-button-prev{left: 20px;background: url("../images/btn_left.png") no-repeat center/contain;}
.warnSwiper .swiper-button-next{right: 20px;background: url("../images/btn_right.png") no-repeat center/contain;}

.closeBtn{position: absolute;width: 1.5rem;height: 1.5rem;background: url("../images/closeBtn.png") no-repeat center/70%;top: -.75rem;right: -.75rem;border: 1px solid #fff;border-radius: 50%;}


/* 弹窗蒙城 */
#layer{position: fixed;top: 0;right: 0;bottom: 0;left: 0;background-color: rgba(31,107,123,.95);display: none;}


/* 自定义下拉框 */
.sel-boxone{display: inline-block; border: 1px solid #ccc; font-size: 14px;  color: #777;border-radius: 5px;-webkit-border-radius: 5px;-moz-border-radius: 5px;position: relative;}
.sel-boxone input{ border: 0; height: 100%;font-size: 14px;  color: #fff;line-height: 26px;padding:0 .6rem;display: block;float: left; background: url('../images/icon_down.png') 94% center no-repeat;}
.sel-boxone .sel-list{position: absolute;top: 100%;right:0;left: 0; width: 100%; border: 1px solid #ccc;border-top:none;background-color: #fff;}
.sel-boxone .sel-list li{height: 1.6rem;line-height: 1.6rem;color: #333;cursor: pointer;}
.sel-boxone .sel-list li:hover{color: #fff;background: #008fe7;}












