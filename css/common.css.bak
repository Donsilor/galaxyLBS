@charset "utf-8";
/* 深圳市静江网络科技有限公司 */
*,*:before,*:after {-webkit-box-sizing:border-box; box-sizing:border-box;margin:0; padding:0;}
:root {font-size: calc(.8vw + .1vh);line-height: 1.5;}
html{-ms-text-size-adjust:100%;-webkit-text-size-adjust: 100%;font-family: "PingFang SC UltraLight",PingFangSC-Light,"Microsoft YaHei",Tahoma,"Hiragino Sans GB","WenQuanYi Micro Hei",sans-serif;}
body{position:relative;color:#fff;font-size: 1rem;}
label{cursor: pointer;}
input{outline: none;border:none;background: none;color: #fff;}
input::-webkit-input-placeholder{color:#a5a8ac;}
input:-moz-placeholder{color:#a5a8ac;}
input::-moz-placeholder{color:#a5a8ac;}
input:-ms-input-placeholder{color:#a5a8ac;}
input[type="button"],button,select{cursor: pointer;border: none;outline: none;}
input[type="checkbox"]{-webkit-appearance: none;-moz-appearance: none;appearance: none;width: .875rem;height: .875rem;border: 2px solid #fff;border-radius: 3px;margin: 2px 0;vertical-align: text-bottom;cursor: pointer;}
input[type="checkbox"]:checked{background: url("../images/checkImg.jpg") no-repeat center/.375rem;}
textarea{outline: none;}
li{list-style-type:none;}
img{border:none;}
a{text-decoration:none; color:#fff;transition: all .3s;cursor: pointer;}
.clf{zoom:1}
.clf:after,.w:after,ul:after{content:"";display:block;height:0;clear:both;overflow: hidden;}
.fl{float: left;}
.fr{float: right;}
/*.w{width: 1200px;max-width: 100%;margin: auto;}*/
/* 初始化样式结束 */





